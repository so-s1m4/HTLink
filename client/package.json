{
  "name": "test",
  "version": "1.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve --host 0.0.0.0 ",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "svg": "svg-generator"
  },
  "svgGenerator": {
    "outputPath": "./src/app/shared/svgs",
    "srcPath": "./src/assets/svg",
    "svgoConfig": {
      "plugins": [
        "removeDimensions",
        "cleanupAttrs",
        {
          "name": "convertColors",
          "params": {
            "currentColor": true
          }
        }
      ]
    }
  },
  "prettier": {
    "printWidth": 100,
    "singleQuote": true,
    "overrides": [
      {
        "files": "*.html",
        "options": {
          "parser": "angular"
        }
      }
    ]
  },
  "private": true,
  "dependencies": {
    "@angular/common": "^21.0.7",
    "@angular/compiler": "21.0.0",
    "@angular/core": "21.0.0",
    "@angular/forms": "^21.0.7",
    "@angular/platform-browser": "^21.0.7",
    "@angular/router": "^21.0.7",
    "@angular/service-worker": "^21.0.7",
    "@ng-icons/core": "^33.0.0",
    "@ng-icons/heroicons": "^33.0.0",
    "@ng-icons/material-icons": "^33.0.0",
    "emoji-toolkit": "^9.0.1",
    "marked": "^16.4.2",
    "ngx-editor": "^19.0.0-beta.1",
    "ngx-markdown": "^20.1.0",
    "prismjs": "^1.30.0",
    "rxjs": "~7.8.0",
    "service-worker": "^0.0.0",
    "tslib": "^2.3.0",
    "turndown": "^7.2.2",
    "zone.js": "~0.15.0"
  },
  "devDependencies": {
    "@angular/build": "^21.0.0",
    "@angular/cli": "^21.0.5",
    "@angular/compiler-cli": "^21.0.7",
    "@types/jasmine": "~5.1.0",
    "@types/turndown": "^5.0.6",
    "jasmine-core": "~5.9.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.2.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.1.0",
    "typescript": "~5.9.2"
  }
}
